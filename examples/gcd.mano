// Algoritmo de Euclides para MDC (Máximo Divisor Comum)
// Versão recursiva - um dos algoritmos mais antigos da história!

olhaEssaFita mdc(a, b) {
    sePá (b == 0) toma a;
    toma mdc(b, a % b);
}

salve "MDC(48, 18) =";
salve mdc(48, 18);

salve "MDC(100, 35) =";
salve mdc(100, 35);

salve "MDC(17, 23) =";
salve mdc(17, 23);